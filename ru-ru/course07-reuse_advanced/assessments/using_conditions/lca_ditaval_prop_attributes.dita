      <lcAnswerContent>prop, val, and action</lcAnswerContent>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>att, val, and filter</lcAnswerContent>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>att, match, and filter</lcAnswerContent>
     </lcAnswerOption>
