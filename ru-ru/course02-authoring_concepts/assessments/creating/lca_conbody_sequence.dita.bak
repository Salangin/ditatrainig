 <learningAssessmentbody>
  <lcInteraction>
   <lcTrueFalse id="link8">
    <lcQuestion>The elements in the &lt;conbody> have to occur in a specific sequence.</lcQuestion>
    <lcAnswerOptionGroup id="lcAnswerOptionGroup_n52_4jx_dt">
     <lcAnswerOption>
      <lcAnswerContent>False</lcAnswerContent>
      <lcCorrectResponse/>
     </lcAnswerOption>
     <lcAnswerOption>
      <lcAnswerContent>True</lcAnswerContent>
     </lcAnswerOption>
    </lcAnswerOptionGroup>
    <lcFeedbackIncorrect>The &lt;conbody> uses an open content model, that is, the elements do not
     have to follow in strict sequence.</lcFeedbackIncorrect>
   </lcTrueFalse>
  </lcInteraction>
